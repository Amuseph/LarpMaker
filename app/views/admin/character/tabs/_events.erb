<table border = 1>
<tr>
    <th>Name</th>
    <th>Description</th>
    <th>Date</th>
</tr>
<% @character.eventattendances.joins(:event).order('startdate desc').each do |eventattendance| %>
    <tr>
      <td><%= eventattendance.event.name %></td>
      <td><%= eventattendance.event.description %></td>
      <td><%= eventattendance.event.startdate %></td>
    </tr>
  <% end %>
  </table>