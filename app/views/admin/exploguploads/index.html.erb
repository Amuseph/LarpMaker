<div style="padding: 20px" >
<% @result_messages.each do |message| %>
  <li><%= message %></li>
<% end %>

<%= bootstrap_form_with(url: admin_exploguploads_path, local: true) do |f| %>

  <%= f.text_area :explogs, placeholder: 'PlayerID|Name|Description|Amount', label: 'Explog Upload Submission' %>
  <br>
  <button type="submit" class="btn btn-primary">Submit</button>
<% end %>

</div>